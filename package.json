    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "mysql2": "^3.3.1",
    "sequelize": "^6.31.1",
    "sqlite3": "^5.1.6"
  }
}
